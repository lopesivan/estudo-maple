output_precision(5)

% Vs_ -_---------------------------------------------------------------------
disp('Vs_ -----------------------------------------------------------------')
Vs_ = to_rd(10, 0)
[Vs, arg_Vs ]= to_pd(Vs_)

% Z1_ ------------------------------------------------------------------------
disp('')
disp('Z1_ -----------------------------------------------------------------')
R   = 8;
X   = 6;

Z1_  = R + X*j;
[P, Q, S, FP, I, arg_I] = analyze_impedance(Vs_, Z1_)

% Z2_ ------------------------------------------------------------------------;
disp('');
disp('Z2_ -----------------------------------------------------------------');
R   = 3;
X   = 4;

Z2_  = R + X*j;
[P, Q, S, FP, I, arg_I] = analyze_impedance(Vs_, Z2_)

% Zeq_ -----------------------------------------------------------------------;
disp('');
disp('Zeq_ ----------------------------------------------------------------');

Zeq_ = z_p([Z1_, Z2_])
[P, Q, S, FP, I, arg_I] = analyze_impedance(Vs_, Zeq_)

y = abs(I*sin(d2r(arg_I))) -1.5432
x = abs(I*cos(d2r(arg_I)))
sqrt(x*x+y*y)
norm([x,y],2)
% Zc_ ------------------------------------------------------------------------;
disp('');
disp('Zc_ -----------------------------------------------------------------');
R   = 0;
X   = 6.48;

Zc_  = R - X*j;
[P, Q, S, FP, I, arg_I] = analyze_impedance(Vs_, Zc_)

% Zf_ ------------------------------------------------------------------------;
disp('');
disp('Zf_ -----------------------------------------------------------------');
Zf_ = z_p([Zeq_, Zc_]);
[P, Q, S, FP, I, arg_I] = analyze_impedance(Vs_, Zf_)
