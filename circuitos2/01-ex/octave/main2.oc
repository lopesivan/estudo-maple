% main2.oc - main2
% Maintainer: Ivan Lopes

Vs_ = to_rd(10, 0)
[Vs, arg_Vs ]= to_pd(Vs_)

% Z1_ ------------------------------------------------------------------------
R   = 8;
X   = 6;

Z1_  = R + X*j
[Z1, arg_Z1 ] = to_pd(Z1_)

Z_ = Z1_
I_ = Vs_/Z_
[I, arg_I ]= to_pd(I_)

% Z2_ ------------------------------------------------------------------------
R   = 3;
X   = 4;

Z2_  = R + X*j
[Z2, arg_Z2 ] = to_pd(Z2_)

Z_ = Z2_
I_ = Vs_/Z_
[I, arg_I ]= to_pd(I_)

% Zeq_ -----------------------------------------------------------------------
Zeq_ = z_p([Z1_, Z2_])
[Zeq, arg_Zeq ] = to_pd(Zeq_)

Z_ = Zeq_
I_ = Vs_/Z_
[I, arg_I ]= to_pd(I_)

% Zc_ ------------------------------------------------------------------------
R   = 0
X   = 6.48;

Zc_  = R + X*j
[Zc, arg_Zc ] = to_pd(Zc_)

Z_ = Zc_
I_ = Vs_/Z_
[I, arg_I ]= to_pd(I_)

% Zf_ ------------------------------------------------------------------------
Zf_ = z_p([Zeq_, Zc_])
[Zf, arg_Zf ] = to_pd(Zf_)

Z_ = Zc_
I_ = Vs_/Z_
[I, arg_I ]= to_pd(I_)
